// sample_graph.g — Graphviz-style graph (DOT-like)
graph G {
  // simple undirected triangle
  A -- B;
  B -- C;
  C -- A;

  // node attributes
  A [label="Node A", color=blue];
  B [label="Node B", color=red];
  C [label="Node C", color=green];
}
// sample_grammar.g — Bison/Yacc-style grammar example
%{
#include <stdio.h>
%}

%token NUMBER
%left '+' '-'
%left '*' '/'

%%
input:
    /* empty */
  | input line
  ;

line:
    '\n'
  | expr '\n' { printf("Result: %d\n", $1); }
  ;

expr:
    NUMBER                { $$ = $1; }
  | expr '+' expr         { $$ = $1 + $3; }
  | expr '-' expr         { $$ = $1 - $3; }
  | expr '*' expr         { $$ = $1 * $3; }
  | expr '/' expr         { $$ = $1 / $3; }
  ;
%%

int main(void) {
  return yyparse();
}

int yyerror(char *s) {
  fprintf(stderr, "Error: %s\n", s);
  return 0;
}
( sample_gcode.g — simple G-code to mill a 10mm square )
G21         ; set units to millimeters
G90         ; absolute positioning
G1 Z5.000 F300   ; lift to safe height
G0 X0 Y0        ; rapid move to origin
G1 Z-1.000 F100 ; plunge into material
G1 X10.000 Y0.000 F400
G1 X10.000 Y10.000
G1 X0.000 Y10.000
G1 X0.000 Y0.000
G1 Z5.000 F300  ; retract
M30             ; end of program
